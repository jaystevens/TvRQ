set(@PROJECT_NAME@_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

set_and_check(@PROJECT_NAME@_INCLUDE_DIR "@PACKAGE_INSTALL_INCLUDE_DIR@")
set_and_check(@PROJECT_NAME@_LIB_DIR "@PACKAGE_INSTALL_LIB_DIR@")

add_library(@PROJECT_NAME@::@PROJECT_NAME@ INTERFACE IMPORTED)
set_target_properties(@PROJECT_NAME@::@PROJECT_NAME@ PROPERTIES
	INTERFACE_INCLUDE_DIRECTORIES "${@PROJECT_NAME@_INCLUDE_DIR}"
	INTERFACE_LINK_DIRECTORIES "${@PROJECT_NAME@_LIB_DIR}"
	INTERFACE_LINK_LIBRARIES "@PROJECT_NAME@"
)


check_required_components(@PROJECT_NAME@)
